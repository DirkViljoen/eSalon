extends ../layouts/master_login
include ../../mixins/mixins

block content
    form.form-signin.form-horizontal(action='/login',method='post')
        h2 Login details
        br
        +form-inputbox('text', 'liUsername', 'Username:', 'Enter your username', '')
        +form-inputbox('password', 'liPassword', 'Password:', 'Enter your password', '')
        +form-submit('Login', '/booking', 'btnLogin')
